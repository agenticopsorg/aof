{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AOF Desktop",
  "version": "0.1.0",
  "identifier": "io.aof.desktop",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "./ui/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
"icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png"
    ],
    "resources": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "Agent Operating Framework Desktop Client",
    "longDescription": "A powerful desktop client for managing and orchestrating AI agents through the Agent Operating Framework"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "AOF Desktop",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "center": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "devCsp": null,
      "freezePrototype": false,
      "dangerousDisableAssetCspModification": false
    }
  },
  "plugins": {}
}
